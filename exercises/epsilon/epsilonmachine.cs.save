public static class epsilonmachine{
	static int Main(){
		double x=1; while(1+x!=1){x/=2;} x*=2;
		System.Console.Write("My machine epsilon with double precision is {0}\n",x);
		float y=1f; while((float)(1f+y) != 1f) {y/=2f;} y*=2;
		System.Console.Write("My machine epsilon with float precision is {0}\n",y);
		return 0;
	}
}
